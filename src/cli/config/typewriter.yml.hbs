# RudderStack Typewriter Configuration (https://rudderstack.com/docs/typewriter)
# Just run `npx typewriter@next` to re-generate a client with the latest versions of these events.
{{#if scripts.token}}

scripts:
  after: {{scripts.token}}
{{/if}}

# You can find more documentation on configuring this client in the RudderStack docs.
# See: https://rudderstack.com/docs/typewriter
client:
  {{#each client}}
  {{@key}}: {{this}}
  {{/each}}

trackingPlans:
  {{#each trackingPlans}}
  # Tracking Plan: {{name}}
  # https://app.rudderstack.com/tracking-plans/{{id}}
  - id: {{id}}
    workspaceSlug: {{workspaceSlug}}
    path: {{path}}

  {{/each}}
